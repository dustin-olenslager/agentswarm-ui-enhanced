{
  "name": "agentswarm",
  "version": "0.1.0",
  "private": true,
  "description": "Multi-agent coding harness",
  "scripts": {
    "build": "turbo run build",
    "clean": "turbo run clean",
    "typecheck": "turbo run typecheck",
    "stagehand:record": "node scripts/stagehand-record.mjs",
    "stagehand:record:hook": "node scripts/stagehand-record.mjs --trigger=post-merge",
    "stagehand:hooks:install": "bash scripts/install-stagehand-hooks.sh"
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "turbo": "^2.0.0",
    "typescript": "^5.5.0"
  },
  "packageManager": "pnpm@9.0.0",
  "dependencies": {
    "@agentswarm/core": "workspace:*",
    "@browserbasehq/stagehand": "^3.0.8",
    "@modelcontextprotocol/sdk": "^1.26.0",
    "zod": "^4.3.6"
  }
}
